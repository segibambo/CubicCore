
@model Cubic.Data.ViewModel.FrameworkSetupViewModel
@{
    ViewData["Title"] = "CurrentConfig";
    Layout = "~/Views/Shared/_APPCubic.cshtml";
}


<div class="container">
    <div class="row">
        <div class="col-xs-12">
            <div class="page-title-box">
                <h4 class="page-title">Current Settings For Portal Configuration </h4>
                <ol class="breadcrumb p-0 m-0">
                    <li>
                        <a>App Setup</a>
                    </li>
                </ol>
                <div class="clearfix"></div>
            </div>
        </div>
    </div>
    <!-- end row -->


    <div class="row">
        <div class="col-sm-12">
            <div class="card-box">
                <div class="form-horizontal">
                    <div class="row">
                        <div class="col-md-6">
                            <div class="form-group">
                                @Html.LabelFor(model => model.DefaultSetting.FrameworkName, new { htmlAttributes = new { @class = "col-md-2 control-label" } })
                                <div class="col-md-10">
                                    @Html.EditorFor(model => model.DefaultSetting.FrameworkName, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
                                </div>
                            </div>
                        </div>

                        <div class="col-md-6">
                            <div class="form-group">
                                @Html.LabelFor(model => model.DefaultSetting.DefaultDatabaseEngine, new { htmlAttributes = new { @class = "col-md-2 control-label" } })
                                <div class="col-md-10">
                                    @Html.EditorFor(model => model.DefaultSetting.DefaultDatabaseEngine, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
                                </div>
                            </div>
                        </div>
                    </div>


                    <div class="row">
                        <div class="col-md-6">
                            <div class="form-group">
                                @Html.LabelFor(model => model.DefaultSetting.FrameworkVersion, new { htmlAttributes = new { @class = "col-md-2 control-label" } })
                                <div class="col-md-10">
                                    @Html.EditorFor(model => model.DefaultSetting.FrameworkVersion, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
                                </div>
                            </div>
                        </div>

                        <div class="col-md-6">
                            <div class="form-group">
                                @Html.LabelFor(model => model.DefaultSetting.TargetServer, new { htmlAttributes = new { @class = "col-md-2 control-label" } })
                                <div class="col-md-10">
                                    @Html.EditorFor(model => model.DefaultSetting.TargetServer, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
               


                <div class="row">
                    <div class="col-md-6">

                        <div class="form-horizontal">

                            
                            <div class="form-group">
                                @Html.LabelFor(model => model.DefaultSetting.IOC, new { htmlAttributes = new { @class = "col-md-2 control-label" } })
                                <div class="col-md-10">
                                    @Html.EditorFor(model => model.DefaultSetting.IOC, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
                                </div>
                            </div>
                            <div class="form-group">
                                @Html.LabelFor(model => model.DefaultSetting.FrameworkDescription, new { htmlAttributes = new { @class = "col-md-2 control-label" } })
                                <div class="col-md-10">
                                    @Html.TextAreaFor(model => model.DefaultSetting.FrameworkDescription, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
                                </div>
                            </div>

                        </div>
                    </div>

                    <div class="col-md-6">
                        <div class="form-horizontal">

                            
                            <div class="form-group">
                                @Html.LabelFor(model => model.DefaultSetting.Developed, new { htmlAttributes = new { @class = "col-md-2 control-label" } })
                                <div class="col-md-10">
                                    @Html.EditorFor(model => model.DefaultSetting.Developed, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
                                </div>
                            </div>
                            <div class="form-group">
                                @Html.LabelFor(model => model.DefaultSetting.Graphic, new { htmlAttributes = new { @class = "col-md-2 control-label" } })
                                <div class="col-md-10">
                                    @Html.EditorFor(model => model.DefaultSetting.Graphic, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
                                </div>
                            </div>
                        </div>

                    </div>


                </div>


            </div>
        </div>


        @using (Html.BeginForm())
    {
        <div class="box-footer pull-right">
            <input type="submit" value="<< Back" name="backButton" class="btn btn-info" /> &nbsp; <input type="submit" value="Next >>" name="nextButton" class="btn btn-primary" />
        </div>
}

    </div>

</div>
